name: TestFlightA
desc: TestFlight账户管理
#!author: NobyDa

http:
  script:
    - match: ^https:\/\/testflight\.apple\.com\/v\d\/(app|account|invite)s\/
      name: TestFlightA
      type: request
      timeout: 30
      require-body: true
      max-size: 3145728
      binary-mode: false

  mitm:
    - "testflight.apple.com"

script-providers:
  TestFlightA:
    url: https://raw.githubusercontent.com/Centralmatrix3/Matrix-io/master/Stash/Scripts/TestFlightA.Js
    interval: 86400